{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-microservice/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nestjs-microservice/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestjs-microservice",
  "projects": {
    "nestjs-microservice": {
      "type": "application",
      "root": "apps/nestjs-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/nestjs-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjs-microservice/tsconfig.app.json"
      }
    },
    "message": {
      "type": "application",
      "root": "apps/message",
      "entryFile": "main",
      "sourceRoot": "apps/message/src",
      "compilerOptions": {
        "tsConfigPath": "apps/message/tsconfig.app.json"
      }
    },
    "notification": {
